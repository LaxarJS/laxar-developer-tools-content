    "babel-preset-es2015": "^6.9.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.0.1",
    "eslint-config-laxarjs": "^0.3.0",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "generator-laxarjs": "~1.5.1",
    "glob": "^7.0.5",
    "img-loader": "^1.3.1",
